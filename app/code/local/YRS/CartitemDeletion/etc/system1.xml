<config>
    <tabs>
        <ctab_config translate="label" module="customtab">
            <label>YRS CustomTab</label>
            <sort_order>100</sort_order>
        </ctab_config>
    </tabs>
    <sections>
        <ctab_options translate="label" module="customtab">
            <label>CustomTab Setting</label>
            <tab>ctab_config</tab>
            <sort_order>1000</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
 
            <groups>
                <ctab_group translate="label" module="customtab">
                    <label>Redirection Setting</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
 
                    <fields>											
                        <customtab_enable translate="label">
							<label>Redirection Enable:</label>
							<comment></comment>
							<frontend_type>select</frontend_type>
							<sort_order>10</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
							<can_be_empty>1</can_be_empty>
                            <source_model>adminhtml/system_config_source_enabledisable</source_model>
						</customtab_enable>											
                        
                    </fields>
                </ctab_group>
            </groups>
        </ctab_options>
    </sections>
</config>
